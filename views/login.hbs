<form class="form-signin">
  <a href="/">
    <h1>DaWebCine</h1>
  </a>
  <label for="inputEmail" class="sr-only">Correo Electrónico</label>
  <input type="email" id="correo" class="form-control" placeholder="Correo Electrónico" required autofocus>
  <br>
  <label for="inputPassword" class="sr-only">Contraseña</label>
  <input type="password" id="pass" class="form-control" placeholder="Contraseña" required>
  <div class="checkbox mb-3">
    <label>
      <a href="./registro">Regístrate aquí</a>
    </label>
  </div>
  <button class="btn btn-lg btn-primary btn-block" type="submit" onclick="login()">Iniciar Sesión</button>
</form>
<script text="text/javascript" src="js/login_functions.js"></script>